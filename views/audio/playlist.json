currentIndex: 0,
  audio: null,
  isPlaying: false,

  get currentTrack() {
    return this.tracks[this.currentIndex];
  },

  init() {
    // Inisialisasi audio saat komponen dimuat
    this.audio = new Audio('./public/audio/' + this.currentTrack.source);
    this.audio.addEventListener('ended', () => this.nextTrack());
  },

  togglePlayPause() {
    if (this.isPlaying) {
      this.audio.pause();
    } else {
      this.audio.play();
    }
    this.isPlaying = !this.isPlaying;
  },

  nextTrack() {
    this.currentIndex = (this.currentIndex + 1) % this.tracks.length;
    this.playTrack();
  },

  prevTrack() {
    this.currentIndex = (this.currentIndex - 1 + this.tracks.length) % this.tracks.length;
    this.playTrack();
  },

  playTrack() {
    this.audio.src = \'./public/audio/\' + this.currentTrack.source;
