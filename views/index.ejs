<%= total_request %>
</p>
</div>
</div>
<!-- Card -->
<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
<div class="p-3 mr-4 text-green-500 bg-yellow-100 rounded-full dark:text-green-100 dark:bg-green-500 fas fa-power-off animate-pulse"></div>
<div>
<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400">
Server Uptime
</p>
<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
<span class="countdown text-lg font-semibold text-gray-700 dark:text-gray-200">
<span id="uphours"></span>h&nbsp;
<span id="upminutes"></span>m&nbsp;
<span id="upseconds"></span>s&nbsp;
</span>
</p>
</div>
</div>
<!-- Card -->
<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
<div class="p-3 mr-4 text-blue-500 bg-blue-100 rounded-full dark:text-blue-100 dark:bg-blue-500 fas fa-server animate-pulse">
</div>
<div>
<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400">Total Endpoint</p>
<p class="text-lg font-semibold text-gray-700 dark:text-gray-200"><%= feature_list %></p>
</div>
</div>
<!-- Card -->
<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
<div class="p-3 mr-4 text-teal-500 bg-teal-100 rounded-full dark:text-teal-100 dark:bg-teal-500 fas fa-clock animate-spin">
</div>
<div>
<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400">Realtime</p>
<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
<span class="countdown font-mono text-lg font-semibold text-gray-700 dark:text-gray-200">
<span id ="hour"></span>:
<span id ="minute"></span>:
<span id ="second"></span>
</span>
<span id="ampm"></span>
</p>
</div>
</div>
</div>
<!-- Card Welcome -->
<div class="grid gap-6 mb-8 md:grid-cols-2">
<div class="min-w-0 p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
<span id="typing" class="mb-4 font-semibold text-gray-600 dark:text-gray-300 typing"></span>
<p class="text-gray-600 dark:text-gray-400 mb-4">
This API is currently under development. If you encounter any bugs or errors, please don't hesitate to contact us. Your feedback is valuable for improving the service.
</p>
</div>
  <div x-data="{ 
    isModalOpen: false, 
    audio: null, 
    searchQuery: '', 
    searchResults: [], 
    currentTrack: null, 
    currentIndex: 0, 
    isLoading: false, 
    playlist: [], 
    initAudio() {
      this.audio = new Audio();
      this.audio.addEventListener('ended', () => this.nextTrack());
    },
    async searchMusic() { 
      if (!this.searchQuery.trim()) return; 
      this.isLoading = true; 
      try { 
        const res = await fetch(`/api/yts?query=${encodeURIComponent(this.searchQuery)}`); 
        const data = await res.json(); 
        this.searchResults = data.results.slice(0, 5);
        this.playlist.push(...data.results.slice(0, 5)); 
      } catch (e) { 
        console.error(e); 
      } 
      this.isLoading = false; 
    }, 
    async playMusic(index) { 
      try { 
        const track = this.playlist[index]; 
        const res = await fetch(`/api/ytmp3?url=${encodeURIComponent(track.url)}`); 
        const data = await res.json(); 
        this.audio.src = data.download.url; 
        this.audio.load();
        this.audio.play(); 
        this.currentTrack = track; 
        this.currentIndex = index; 
        this.isModalOpen = false; 
      } catch (e) { 
        console.error(e); 
      } 
    }, 
    nextTrack() { 
      if (this.playlist.length > 0) { 
        const nextIndex = (this.currentIndex + 1) % this.playlist.length; 
        this.playMusic(nextIndex); 
      } 
    }, 
    prevTrack() { 
      if (this.playlist.length > 0) { 
        const prevIndex = (this.currentIndex - 1 + this.playlist.length) % this.playlist.length; 
        this.playMusic(prevIndex); 
      } 
    },
    togglePlayPause() {
      const buttonIcon = document.getElementById('ngentods');
      if (this.audio.paused) {
        this.audio.play();
        buttonIcon.classList.replace('fa-play', 'fa-pause');
      } else {
        this.audio.pause();
        buttonIcon.classList.replace('fa-pause', 'fa-play');
      }
    }
  }" x-init="initAudio">
    <!-- Player Section -->
    <div class="min-w-0 p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
      <template x-if="currentTrack">
        <div>
          <h2 class="mb-4 font-semibold text-gray-600 dark:text-gray-300"><strong>Now Playing</strong></h2>
          <div class="flex items-center">
            <img :src="currentTrack.image" alt="Cover Art" class="w-12 h-12 rounded-md object-cover mr-4" />
            <div class="flex-1">
              <h3 class="text-gray-600 dark:text-gray-400 mb-4 truncate" x-text="currentTrack.title"></h3>
              <div class="flex mt-2 space-x-4">
                <button @click="prevTrack" class="btn btn-circle btn-primary">
                  <i class="fas fa-step-backward"></i>
                </button>
                <button @click="togglePlayPause" class="btn btn-circle btn-primary">
                  <i id="ngentods" class="fas fa-play"></i>
                </button>
                <button @click="nextTrack" class="btn btn-circle btn-primary">
                  <i class="fas fa-step-forward"></i>
                </button>
              </div>
            </div>
          </div>
        </div>
      </template>
      <template x-if="!currentTrack">
        <p class="text-center text-gray-500">No track playing</p>
      </template>
      <button @click="isModalOpen = true" class="btn btn-active btn-primary mt-4 w-full">Search</button>
    </div>
    <!-- Search Section -->
    <div x-show="isModalOpen" class="fixed inset-0 z-30 flex items-center justify-center bg-black bg-opacity-50">
      <div @click.away="isModalOpen = false" @keydown.escape="isModalOpen = false" class="w-full px-6 py-4 bg-white rounded-lg sm:m-4 sm:max-w-xl dark:bg-gray-800">
        <header class="flex justify-between">
          <h2 class="text-lg font-semibold text-gray-700 dark:text-gray-300">Search Your Fav Songs!</h2>
          <button @click="isModalOpen = false" class="text-gray-400 hover:text-gray-700 dark:hover:text-gray-200">âœ•</button>
        </header>
        <div class="mt-4 flex">
          <input x-model="searchQuery" @keydown.enter="searchMusic" placeholder="Enter song or artist" class="w-full px-4 py-2 border rounded-lg mr-2 focus:ring-purple-300 dark:bg-gray-700 dark:text-white" />
        </div>
        <div class="mt-4">
          <template x-if="isLoading"><span class="loading loading-bars loading-lg"></span></template>
          <template x-if="!isLoading && searchResults.length === 0"><p class="text-gray-500">No results found.</p></template>
          <template x-if="searchResults.length > 0">
            <ul>
              <template x-for="(result, index) in searchResults" :key="result.url">
                <li class="flex items-center justify-between py-2 border-b dark:border-gray-600">
                  <div class="flex items-center space-x-4">
                    <img :src="result.image" alt="" class="w-12 h-12 rounded-md object-cover" />
                    <span class="text-sm text-gray-700 dark:text-gray-300" x-text="result.title"></span>
                  </div>
                  <button @click="
                    const icon = document.querySelector(`#play-icon-${index}`); 
                    if (icon) {
                      await icon.classList.replace('fa-play', 'fa-spinner'); 
                      await icon.classList.add('animate-spin');
                    } 
                    await playMusic(index); 
                    if (icon) {
                      await icon.classList.remove('animate-spin'); 
                      await icon.classList.replace('fa-spinner', 'fa-pause');
                    }
                  " class="btn btn-info btn-circle">
                    <i :id="'play-icon-' + index" class="fas fa-play"></i>
                  </button>
                </li>
              </template>
            </ul>
          </template>
        </div>
      </div>
    </div>
  </div>

</div>
</main>
</div>
</div>
